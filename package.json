{
  "name": "corporate-portal",
  "version": "0.2.0",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=10.15.0",
    "npm": ">=6.4.1",
    "yarn": ">=1.17.3"
  },
  "scripts": {
    "start": "yarn migrate && gulp serve",
    "start:dev": "yarn  && yarn start",
    "start:commands": "node dist/server/commands",
    "start:commands:dev": "nodemon dist/server/commands",
    "start:debug": "cross-env DEBUG=corporate-portal:* yarn start",
    "build": "gulp",
    "copy": "gulp copy",
    "babel": "gulp babel",
    "lint": "npx eslint *.js server --color",
    "lint:watch": "yarn lint -- --watch",
    "fix-js": "eslint-auto-fix \"src/**/.js\" \"config/**/.js\" \"config/*.js\"",
    "beautify": "es-beautifier -u -c .eslintrc.js *.js server config",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _jest -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "migrate": "npx sequelize-cli db:migrate",
    "apidoc": "apidoc -i server/routes -o apidoc/"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es6",
    "jest",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "jest": {
    "automock": false,
    "bail": false,
    "verbose": true,
    "setupFiles": [
      "./jest-setup.js"
    ],
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test|integration).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/dist/"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "activedirectory": "0.7.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "connect-redis": "5.0.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cron": "1.8.2",
    "debug": "^4.1.1",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-joi-validation": "4.0.4-beta.0",
    "express-jwt": "6.0.0",
    "express-session": "1.17.1",
    "express-winston": "4.0.5",
    "gulp": "4.0.2",
    "gulp-load-plugins": "^2.0.4",
    "helmet": "4.1.0",
    "http-status": "^1.4.2",
    "jsonwebtoken": "8.5.1",
    "md5-hex": "3.0.1",
    "method-override": "^3.0.0",
    "moment": "2.27.0",
    "nodemon": "^2.0.4",
    "pg": "8.3.3",
    "pg-hstore": "2.3.3",
    "redis": "3.0.2",
    "sequelize": "6.3.5",
    "supertest": "4.0.2",
    "uuid": "8.3.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-bind": "^7.11.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "apidoc": "0.25.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.2.1",
    "coveralls": "^3.1.0",
    "cross-env": "7.0.2",
    "eslint": "7.8.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-watch": "7.0.0",
    "gulp-apidoc": "^0.2.8",
    "gulp-babel": "8.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.2.5",
    "istanbul": "1.1.0-alpha.1",
    "jest": "26.4.2",
    "jest-fetch-mock": "3.0.3",
    "jsdom": "16.4.0",
    "sequelize-cli": "6.2.0"
  }
}
